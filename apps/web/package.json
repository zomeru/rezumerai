{
  "name": "web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "pnpm with-env next dev --turbopack",
    "build": "pnpm with-env next build --turbopack",
    "start": "next start",
    "check-types": "tsc --noEmit",
    "test": "vitest --run",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "analyze": "ANALYZE=true pnpm build",
    "check": "biome check --write .",
    "clean": "rimraf .next node_modules .turbo coverage",
    "with-env": "dotenv -e ../../.env.local --"
  },
  "dependencies": {
    "@rezumerai/database": "workspace:*",
    "@rezumerai/types": "workspace:*",
    "@rezumerai/ui": "workspace:*",
    "@rezumerai/utils": "workspace:*",
    "@tanstack/react-query": "^5.90.19",
    "@ts-rest/core": "3.53.0-rc.1",
    "@ts-rest/react-query": "3.53.0-rc.1",
    "lucide-react": "^0.562.0",
    "next": "16.1.4",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "uuid": "^13.0.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.12",
    "@faker-js/faker": "^10.2.0",
    "@next/bundle-analyzer": "^16.1.4",
    "@rezumerai/tsconfig": "workspace:*",
