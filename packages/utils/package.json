{
  "name": "@rezumerai/utils",
  "version": "1.0.0",
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./styles": {
      "types": "./dist/styles.d.ts",
      "import": "./dist/styles.mjs",
      "require": "./dist/styles.js"
    },
    "./string": {
      "types": "./dist/string.d.ts",
      "import": "./dist/string.mjs",
      "require": "./dist/string.js"
    },
    "./date": {
      "types": "./dist/date.d.ts",
      "import": "./dist/date.mjs",
      "require": "./dist/date.js"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.mjs",
      "require": "./dist/react.js"
    },
    "./uuid": {
      "types": "./dist/uuid.d.ts",
      "import": "./dist/uuid.mjs",
      "require": "./dist/uuid.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "check-types": "tsc --noEmit",
    "test": "vitest --run",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "check": "biome check --write .",
    "clean": "rimraf node_modules .turbo dist coverage tsconfig.tsbuildinfo tsup.config.bundled*"
  },
  "dependencies": {
    "@rezumerai/types": "workspace:*",
    "@types/react": "^19.0.10",
    "clsx": "^2.1.1",
    "tailwind-merge": "^3.4.0",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.15",
    "@rezumerai/tsconfig": "workspace:*",
    "@rezumerai/vitest-config": "workspace:*",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^25.2.3",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "jsdom": "^28.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
