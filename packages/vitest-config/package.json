{
  "name": "@rezumerai/vitest-config",
  "version": "1.0.0",
  "private": true,
  "files": [
    "dist/**",
    "src/**"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./base": {
      "types": "./dist/base.d.ts",
      "import": "./dist/base.mjs",
      "require": "./dist/base.js"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.mjs",
      "require": "./dist/react.js"
    },
    "./node": {
      "types": "./dist/node.d.ts",
      "import": "./dist/node.mjs",
      "require": "./dist/node.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "check-types": "tsc --noEmit",
    "check": "biome check --write .",
    "clean": "rimraf node_modules .turbo dist tsconfig.tsbuildinfo tsup.config.bundled*"
  },
  "dependencies": {
    "vitest": "^4.0.18"
  },
  "devDependencies": {
    "@biomejs/biome": "2.4.2",
    "@rezumerai/tsconfig": "workspace:*",
    "@types/node": "^25.2.3",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
